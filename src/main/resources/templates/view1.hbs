{{#partial "styles"}}
    <link rel='stylesheet' href='/css/app.css'>
{{/partial}}
{{#partial "content"}}
    <br>
    <br>
    <table class="table table-dark">
        <thead class="head-dark">
        <tr>
            <th scope="col">Ranger's Name</th>
            <th scope="col">Location</th>

            <th scope="col">Time of sighting</th>
        </tr>
        </thead>
        <tbody>
        
        {{#each sightings}}
            <tr>
                <td>{{name}}</td>
                <td>{{location}}</td>

                <td>{{timestamp}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
{{/partial}}
{{> layout.hbs}}